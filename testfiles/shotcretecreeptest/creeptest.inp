** Simulation of creep test (24h) by Neuner et al. 2017 -- nonlocal version

*modelGenerator, generator=planeRectQuad, name=gen
l=100
h=200
nX=10
nY=10
elType=uelCPS4Nonlocal

*material, name=shotLeonNonLocal, id=scdp, statevars=125
**             q1,             q2,             q3,             q4,             nu,             E1,        nKelvin,      kelvinMin,
       42.2000000,     41.0000000,     3.0300e+00,     3.4309e+01,     2.1000e-01,     1.4100e+04,     1.2000e+01,     1.0000e-06,
**   strainShrInf,         TauShr,           kHum,          fcu28,           fcu1,       ratioFcy,       ratioFbu,       ratioFtu,
      -2.0000e-03,     3.5770e+03,     1.0000e+00,     4.080e+01,     1.8560e+01,     1.0000e-01,     1.1600e+00,     1.0000e-01,
**          eCPP1,          eCPP8,         eCPP24,             Df,             Ah,             Ch,             Dh,             As,
      -3.0000e-03,    -1.5000e-03,    -7.0000e-04,     8.5000e-01,     8.0000e-02,     2.0000e+00,     1.0000e-06,     1.5000e+01,
**    nonlocal r.,    nonlocal w.,     soft. mod.,   enableDamage,       dTStatic,       castTime,     timeToDays,
                5,           1.05,     0.00170000,     1.0000e+00,     1.0000e-03,     0.0000e+00,     4.1000e-02,

*section, name=section1, thickness=1.0, material=scdp, type=planeUelUmat
all

*job, name=cps4nljob, domain=2d, solver=NIST

*fieldOutput,
name=Displacement,      elSet=all,           field=displacement, result=U, 
name=DisplacementN,      nSet=all,           field=displacement, result=U, 
name=Nonlocal Damage,   elSet=all,           field=nonlocal damage, result=U, 
**name=Nonlocal DamageN,   nSet=all,           field=nonlocal damage, result=U, 
name=DisplacementTop,   elSet=gen_top,       field=displacement, result=U, saveHistory=True
name=LoadTop,           nSet=gen_bottom,    field=displacement, result=P, saveHistory=True

*output, type=monitor, jobName=cps4nljob
name='displacement,top,vertical,mean',  fieldOutput=DisplacementTop, f(x)='mean(x[:,1])'
name='applied load',                    fieldOutput=LoadTop, f(x)='sum(x[:,1]) / (100*1)'

*output, type=ensight, jobName=cps4nljob,
create=perNode, fieldOutput=Displacement
create=perNode, fieldOutput=Nonlocal Damage

*output, type=meshPlot, jobName=cps4nljob
figure=1, axSpec=121, create=xyData,  y=DisplacementTop, x=time, f(y)='mean(y[:,:,1], axis=1) / 200', label=Neuner et al. (24h), 
figure=1, axSpec=122, create=perNode, fieldOutput=DisplacementN, f(x)='x[:,1]', label=U

*configurePlots
figure=1, axSpec=121, flipY=True, yLabel=$\\varepsilon\\thinspace$(-), xLabel=$t\\thinspace$(h)
figure=1, axSpec=122, aspect=equal

*step, jobName=cps4nljob, maxInc=1e-1, minInc=1e-6, maxNumInc=1000, maxIter=25, stepLength=24
dirichlet, name=bottom,  nSet=gen_bottom, field=displacement, 2=0.0
dirichlet, name=corner,  nSet=gen_leftBottom, field=displacement, 2=0.0, 1=0

*step, jobName=cps4nljob, maxInc=2e-2, minInc=1e-1, maxNumInc=1000, maxIter=25, stepLength=0.01
distributedload, name=dload, surface=gen_top, magnitude=3.9, type=pressure, f(t)=t

*step, jobName=cps4nljob, maxInc=5e-3, minInc=1e-6, maxNumInc=1000, maxIter=25, stepLength=1344
